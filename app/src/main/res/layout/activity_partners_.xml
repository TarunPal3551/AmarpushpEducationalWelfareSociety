                        android:layout_alignParentStart="true"
                        android:layout_below="@+id/imageView13"
                        android:layout_marginStart="18dp"
                        android:text="Vishal International School Noida Extension, Noida"
                        android:textColor="@android:color/black"
                        android:layout_alignParentLeft="true"
                        android:layout_marginLeft="18dp" />

                    <ImageView
                        android:id="@+id/imageView13"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignStart="@+id/textView49"
                        android:layout_below="@+id/textView45"
                        android:src="@drawable/partner5"
                        android:layout_alignLeft="@+id/textView49" />

                    <TextView
                        android:id="@+id/textView51"
                        android:layout_width="155dp"
                        android:layout_height="wrap_content"
                        android:layout_alignStart="@+id/textView50"
                        android:layout_below="@+id/imageView14"
                        android:layout_marginTop="-88dp"
                        android:text="Blue Bells School, Allahabad"
                        android:textColor="@android:color/black"
                        android:layout_alignLeft="@+id/textView50" />

                    <TextView
                        android:id="@+id/textView49"
                        android:layout_width="184dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignTop="@+id/textView50"
                        android:text="Bhartiya Shaikshik Evam Sanskritik Sansthan, Allahabad"
                        android:textColor="@android:color/black"
                        android:layout_alignParentRight="true" />
                </RelativeLayout>
            </RelativeLayout>
        </android.support.v4.widget.NestedScrollView>
    </RelativeLayout>
</RelativeLayout>