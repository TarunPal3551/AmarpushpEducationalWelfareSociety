                    android:textStyle="bold"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="23dp" />

                <TextView
                    android:id="@+id/textView44"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/imageView9"
                    android:layout_alignStart="@+id/textView47"
                    android:layout_marginBottom="-37dp"
                    android:text="IT Partner"
                    android:textColor="@color/amarpushpapp"
                    android:textSize="18sp"
                    android:textStyle="bold"
                    android:layout_alignLeft="@+id/textView47" />

                <TextView
                    android:id="@+id/textView47"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_below="@+id/imageView9"
                    android:layout_marginEnd="67dp"
                    android:text="G Systems"
                    android:textColor="@android:color/black"
                    android:layout_marginRight="67dp"
                    android:layout_alignParentRight="true" />

                <TextView
                    android:id="@+id/textView46"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentStart="true"
                    android:layout_below="@+id/imageView9"
                    android:layout_marginStart="51dp"
                    android:text="92.7 Big Fm"
                    android:textColor="@android:color/black"
                    android:layout_alignParentLeft="true"
                    android:layout_marginLeft="51dp" />

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="358dp"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentStart="true"
                    android:layout_alignParentLeft="true">

                    <ImageView
                        android:id="@+id/imageView11"
                        android:layout_width="188dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true"
                        android:layout_alignTop="@+id/imageView14"
                        android:src="@drawable/partner3"
                        android:layout_alignParentRight="true" />

                    <TextView
                        android:id="@+id/textView45"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
